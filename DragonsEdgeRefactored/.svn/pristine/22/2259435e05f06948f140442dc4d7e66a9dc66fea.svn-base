package net.kleopi.Engine.EventManagement.GameEvents;

public class PackageReceivedEvent extends GameEvent {
	private Object updateObject;

	public PackageReceivedEvent(Object pkg) {
		this.setUpdateObject(pkg);
	}

	public Object getUpdateObject() {
		return updateObject;
	}

	public void setUpdateObject(Object updateObject) {
		this.updateObject = updateObject;
	}
}
